{
  "name": "sudoku-accessoriser",
  "version": "1.0.0",
  "description": "A website to make sudoku puzzles more accessible",
  "scripts": {
    "dev": "parcel index.html",
    "build": "parcel build index.html --public-url ./",
    "clean": "rm -rf dist .parcel-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write src/ tests/ *.js *.json *.md",
    "format:check": "prettier --check src/ tests/ *.js *.json *.md",
    "quality": "npm run lint && npm run format:check && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/sudoku-accessoriser.git"
  },
  "keywords": [
    "sudoku",
    "accessibility",
    "puzzle"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "parcel": "^2.10.0",
    "prettier": "^3.6.2"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ]
}
